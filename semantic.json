{
  "base": "/",
  "autoInstall": true,
  "version": "2.8.8",
  "paths": {
    "source": {
      "config": "styles/src/theme.config",
      "definitions": "styles/src/definitions/",
      "site": "styles/src/site/",
      "themes": "styles/src/themes/"
    },
    "output": {
      "packaged": "semantic/",
      "uncompressed": "semantic/components/",
      "compressed": "semantic/components/",
      "themes": "semantic/themes/"
    },
    "clean": "semantic/"
  },
  "permission": "744",
  "rtl": false,
  "components": ["site", "button", "container",  "header", "icon", "input", "label",  "form", "grid", "menu", "message",  "accordion", "checkbox", "toast", "emoji"],
  "individuals": ["form",  "emoji"]
}
